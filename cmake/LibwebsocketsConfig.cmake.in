# - Config file for the Libevent package
# It defines the following variables
#  LIBWEBSOCKETS3_INCLUDE_DIRS - include directories for FooBar
#  LIBWEBSOCKETS3_LIBRARIES    - libraries to link against

# Get the path of the current file.
get_filename_component(LWS_CMAKE_DIR "${CMAKE_CURRENT_LIST_FILE}" PATH)

# Set the include directories.
set(LIBWEBSOCKETS3_INCLUDE_DIRS "@LWS__INCLUDE_DIRS@")

# Include the project Targets file, this contains definitions for IMPORTED targets.
include(${LWS_CMAKE_DIR}/Libwebsockets3Targets.cmake)

# IMPORTED targets from Libwebsockets3Targets.cmake
set(LIBWEBSOCKETS3_LIBRARIES websockets3 websockets3_shared)

